%h1 New user

- form_for( @user ) do |f|
  = f.error_messages
  %p
    = f.label :email
    = f.text_field :email

  %p
    = f.label :password
    = f.password_field :password

  %p
    = f.label :password_confirmation
    = f.password_field :password_confirmation

  %p
    = f.submit 'Create'

= link_to 'Back', users_path
